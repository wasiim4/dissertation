{
    "name": "ken/flash-me",
    "description": "Simple flash alert for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wahyu8",
            "email": "wahyu.dhiraashandy8@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Ken\\FlashMe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ken\\FlashMe\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Ken\\FlashMe\\FlashMeServiceProvider"
            ]
        }
    }
}
